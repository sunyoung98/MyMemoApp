<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.superdroid.test.mymemo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/pencil"
        android:label="@string/app_name"
        android:roundIcon="@drawable/pencil"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DialogMemo"
            android:theme="@android:style/Theme.Holo.Dialog"/>
        <activity
            android:name=".DialogModifyCheckList"
            android:theme="@android:style/Theme.Holo.Dialog" />
        <activity
            android:name=".PopupCheckListActivity"
            android:theme="@android:style/Theme.Holo.Dialog" />
        <activity android:name=".ModifyMemoActivity" />
        <activity android:name=".ReadMemoActivity" />
        <activity android:name=".AddMemoActivity" />
        <activity android:name=".CheckListActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>